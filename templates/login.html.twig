<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        <meta http-equiv="x-ua-compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="robots" content="index,follow">
        <meta property="og:title" content="Concièrgerie">
        <meta property="og:site_name" content="Concièrgerie">
        <meta name="twitter:card" content="summary">
        <meta name="google" content="notranslate">
        <meta name="apple-mobile-web-app-capable" content="no">
        {% block stylesheets %}
            <link rel="stylesheet" type="text/css" href="{{ asset('sass/css/styles.slider.css') }}" media="all">
            <link rel="stylesheet" type="text/css" href="{{ asset('sass/css/jquery-ui-1.11.css') }}" media="all">
            <link rel="stylesheet" type="text/css" href="{{ asset('sass/css/styles.css') }}" media="all">
            <link rel="stylesheet" type="text/css" href="{{ asset('sass/css/styles.responsive.css') }}" media="all">
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}
        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
            <script src="{{ asset('js/libs/jquery.min.js') }}"></script>
            <script src="{{ asset('js/jquery.browser.js') }}"></script>
            <script src="{{ asset('js/placeholder.js') }}"></script>
            <script src="{{ asset('js/jquery-ui-1.11.2.min.js') }}"></script>
            <script src="{{ asset('js/matchMedia.js') }}"></script>
            <script src="{{ asset('js/matchMedia.addListener.js') }}"></script>
            <script src="{{ asset('js/enquire.js') }}"></script>
            <script src="{{ asset('js/unveil.js') }}"></script>
            <script src="{{ asset('js/script.js') }}"></script>
        {% endblock %}
        <meta property="og:locale" content="fr_FR.UTF-8">
        <meta property="og:type" content="website">
        <meta property="og:description" content="">
        <meta name="twitter:card" content="summary">
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;700&display=swap" rel="stylesheet">
        <link href="https://fonts.googleapis.com/css2?family=Heebo&display=swap" rel="stylesheet">
        <link rel="canonical" href="http://dev-micropolis.dyndns.org/">
    </head>
    <body>
        <main>
            {% block body %}{% endblock %}
        </main>
    </body>
</html>
